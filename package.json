{
  "name": "tf-hub",
  "version": "1.1.0",
  "author": "",
  "description": "",
  "license": "MIT",
  "private": true,
  "main": "server.js",
  "scripts": {
    "serve:dev": "npm run dev",
    "dev": "node ./node_modules/nodemon/bin/nodemon.js --inspect --watch app --watch bootstrap --watch config --watch .env -x node server.js",
    "build": "node ./ace nuxtbuild",
    "start": "nodemon --inspect --watch app --watch bootstrap --watch config --watch .env -x node server.js",
    "precommit": "npm run lint",
    "lint": "prettier --check ./resources/**/*.{js,vue} ./app/**/*.js ./database/**/*.js ./start/**/*.js && eslint --max-warnings 0 --ext .js,.vue resources/ app/",
    "lint:fix": "prettier --write ./resources/**/*.{js,vue} ./app/**/*.js ./database/**/*.js ./start/**/*.js && eslint --fix --max-warnings 0 --ext .js,.vue resources/ app/",
    "google:accounts:sync": "node app/Services/GoogleAccountsSyncScript.js",
    "slack:bot": "node app/Services/SlackBot.js",
    "message:scheduler": "node app/Services/MessageScheduler.js"
  },
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/ally": "^2.1.3",
    "@adonisjs/auth": "^3.1.0",
    "@adonisjs/bodyparser": "^2.0.9",
    "@adonisjs/cli": "^5.0.9",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.5",
    "@adonisjs/framework": "^5.0.13",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.2.1",
    "@adonisjs/session": "^1.0.29",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/validator": "^5.0.6",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.12.2",
    "@sendgrid/mail": "^6.5.5",
    "@slack/events-api": "^2.3.4",
    "@slack/web-api": "^5.12.0",
    "acorn": "^6.4.2",
    "adonis-acl": "^1.1.1",
    "adonis-swagger": "^1.2.9",
    "aws-sdk": "^2.767.0",
    "axios": "^0.19.2",
    "cross-env": "^5.2.1",
    "crypto": "^1.0.1",
    "date-fns": "^1.30.1",
    "dotenv": "^8.2.0",
    "google-auth-library": "^6.1.0",
    "handlebars": "^4.7.6",
    "minimist": "^1.2.5",
    "moment": "^2.29.0",
    "mysql": "^2.18.1",
    "node-forge": "^0.10.0",
    "lodash": "^4.17.20",
    "node-google-calendar": "^1.1.1",
    "node-schedule": "^1.3.2",
    "nuxt": "^2.14.6",
    "youch": "^2.1.1"
  },
  "standard": {
    "global": [
      "use",
      "make"
    ]
  },
  "autoload": {
    "App": "./app"
  },
  "devDependencies": {
    "@mdi/font": "^4.9.95",
    "@nuxtjs/device": "^1.2.7",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "eslint-webpack-plugin": "^2.1.0",
    "nodemon": "^2.0.4",
    "prettier": "2.1.2",
